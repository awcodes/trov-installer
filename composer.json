{
    "name": "trov/installer",
    "description": "Trov application installer.",
    "keywords": [
        "trov",
        "laravel",
        "filament"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^5.0|^6.0",
        "symfony/process": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "bin": [
        "bin/trov"
    ],
    "autoload": {
        "psr-4": {
            "Trov\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trov\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}